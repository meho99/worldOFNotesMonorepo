{
  "name": "@won/web-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.1.2",
    "@testing-library/user-event": "^12.2.2",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.11",
    "dotenv": "^8.2.0",
    "find-up": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-scripts": "3.4.0",
    "redux": "^4.0.5",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx,js}\"",
    "lint-fix": "eslint \"{src,test}/**/*.{ts,tsx,js}\" --fix",
    "build": "react-scripts build",
    "test": "jest",
    "test-update": "jest -u",
    "eject": "react-scripts eject",
    "check-types": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": 1,
      "quotes": [
        2,
        "single",
        "avoid-escape"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.5",
    "babel-jest": "^24.9.0",
    "jest": "24.9.0",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.3"
  }
}
